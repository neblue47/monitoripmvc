
.linedwrap {
	border: 1px solid #c0c0c0;
	padding: 0 0 2px 0;
	
}
.contor textarea{
	max-height: 164px !important;
}
.linedtextarea {
	padding: 0px;
	margin: 0px;
}
.linedtextarea textarea, .linedwrap .codelines .lineno {
	font-size: 10pt;
	font-family: monospace;
	line-height: normal !important;
}
.linedtextarea textarea {
	padding-right:0.3em;
	padding-top:0;
	outline: none;
	border: 0;
}
.linedwrap .lines {
	margin-top: 0px;
	max-width: 30px;
	min-height: 190px;
	float: left;
	overflow: hidden;
	border-right: 1px solid #c0c0c0;
	margin-right: 1px;
}
.linedwrap .codelines {
	padding-top: 5px;
}
.linedwrap .codelines .lineno {
	color:#AAAAAA;
	padding-left: 0.8em;
	padding-top: 0.0em;
	text-align: left;
	white-space: nowrap;
}
.linedwrap .codelines .lineselect {
	color: red;
}