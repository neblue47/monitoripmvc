
/* Requisitar Exames*/

.marcarConsulta{
	width: auto;
	height: auto;
	overflow: hidden;
	float: left;
}
#marcarConsultaform fieldset{
	width: 530px;
	border: solid 1px #ddd;
	border-radius: 8px;
	padding: 9px 15px 20px;
	border: solid 1px #ddd;
}
#marcarConsultaform legend{
	font-family: 'Open Sans', sans-serif;
	font-size: 17px;
	font-weight: bold;
	padding: 7px;	    
}
#consGeral{
	height: auto;
	overflow: hidden;
	display: block;
}
.consPerfil label{
	font-size: 14px;
	color: #444;
	text-align: right;
	font-weight: bold;
	float: left;
	padding: 9px 0;
  	margin-bottom: 8px;
  	min-width: 140px;
  	border:solid 0px red
}

.consPerfil input[type=text] {
  color: #666;
  font-size: 14px;
  padding: 8px 12px;
  font-weight: normal;
  width: 350px;
  margin-bottom: 8px;
  background: #F2F2F2;
  border: solid 1px #ccc;
  float: right;
}
.consSelect label{
	font-size: 14px;
	color: #444;
	font-weight: bold;
	padding: 6px 0;
	text-align: right;
	min-width: 140px;
    margin-bottom: 12px;
	float: left;
}
.consSelect select{  
	width: 376px;
	line-height: 35px;
  	min-height: 35px;
  	border: 1px solid #ECECFB;
  	padding: 6px 10px;
    margin-bottom: 12px;
    float: right;
}

/*Table Left css*/
#marcarConsultaform table.cruises { 
    font-family: 'Open Sans', sans-serif;
    font-size: 13px;
	font-weight: bold;
	cellspacing: 0; 
	border-collapse: collapse; 
    width: 471px;    
    }
#marcarConsultaform table.cruises tbody { 
    top: 2.7em;
    height: 182px; 
    width: 530px; 
    overflow: auto; 
    position: absolute;
}
#marcarConsultaform table.cruises td { 
    border-left: 1px solid #999; 
    border-top: 1px solid #999;  
    border: 1px solid #ccc;  
	padding: 9.2px 4px;
	text-align: center;
	font-size: 12px;
	font-weight: bold;
	background: #F2F2F2; 
	overflow: hidden;
}
#marcarConsultaform table.cruises tr:first-child td {
    border-top: none;
}
#marcarConsultaform table.cruises th { 
    border: 1px solid #999; 
	padding: 8px 4px;
	background: #646473;
	color: white;
    }
#marcarConsultaform table.cruises td { 
  	background: #eee; 
  	overflow: hidden; 
}
#marcarConsultaform div.scrollableContainer { 
    position: relative; 
    width: 530px; 
    padding-top: 0px;     
    border: 0px solid #999;
    background: #646473;
}
#marcarConsultaform div.scrollingArea { 
    height: 217px; 
    overflow: auto; 
}
#marcarConsultaform table.scrollable thead tr {
    left: 0px; 
    top: 0;
    position: absolute;  
}
#marcarConsultaform table.cruises .analise  div { width: 225px; }
#marcarConsultaform table.cruises .preco    div { width: 140px; }
#marcarConsultaform table.cruises .accao    div { width: 120px;  text-align:center; }

#marcarConsultaform .btns-consulta{
	float: right;
	right: 18px;
	top: 10px;
	margin-bottom: 20px;
	position: relative;
}
#marcarConsultaform input.btn-marcar-consulta{
	padding: 8px 12px;
	color: #222;
	font-size: 13px;
	font-weight: normal;
	font-variant: small-caps;
	text-indent: 20px;
	border: solid 1px #ccc;
	margin: 0 10px 0 0;
	background: url('../img/microscope-icon2.png') #F2F2F2;
	background-repeat: no-repeat;
	background-size: 26px;
	background-position: 5px;
	border-radius: 3px;
	cursor: pointer;
}
#marcarConsultaform input.btn-limpar-consulta{
	padding: 8px 12px;
	color: #222;
	font-size: 13px;
	font-weight: normal;
	font-variant: small-caps;
	text-indent: 21px;
	border: solid 1px #ccc;
	margin: 0;
	background: url('../img/Eraser-icon.png') #F2F2F2;
	background-repeat: no-repeat;
	background-size: 24px;
	background-position: 7px;
	border-radius: 3px;
	cursor: pointer;
}



