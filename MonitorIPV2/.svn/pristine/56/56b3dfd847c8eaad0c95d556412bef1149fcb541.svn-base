
.marcarExames{
	width: auto;
	height: auto;
	overflow: hidden;
	float: left;
}
#marcarExamesform fieldset{
	width: 530px;
	border: solid 1px #ddd;
	border-radius: 2px;
	padding: 6px 15px 16px;
	border: solid 1px #ddd;
}
#marcarExamesform legend{
	font-family: 'Open Sans', sans-serif;
	font-size: 17px;
	font-weight: bold;
	padding: 7px;	    
}

/* Tipo de Plano*/

.consSelect label{
	font-size: 14px;
	color: #444;
	font-weight: bold;
	padding: 8px 0 5px;
	text-align: right;
	min-width: 140px;
	margin-right: 10px;
}
.consSelect select{  
	width: 396px;
	line-height: 30px;
  	min-height: 30px;
  	border: 1px solid #aaa;
  	padding: 6px 10px;
}

/* Lista de consultas*/

/*Table Left css*/
#marcarExamesform .scrollableContainer{
	margin-top: 20px;
}
#marcarExamesform table.cruises { 
    font-family: 'Open Sans', sans-serif;
    font-size: 13px;
	font-weight: bold;
	cellspacing: 0; 
	border-collapse: collapse; 
    width: 471px;   
    }
#marcarExamesform table.cruises tbody { 
    top: 2.7em;
    height: 302px; 
    width: 530px; 
    overflow: auto; 
    position: absolute;
}
#marcarExamesform table.cruises td { 
    border-left: 1px solid #999; 
    border-top: 1px solid #999;  
    border: 1px solid #ccc;  
	padding: 9.2px 4px;
	text-align: center;
	font-size: 12px;
	font-weight: bold;
	background: #F2F2F2; 
	overflow: hidden;
}
#marcarexamesform table.cruises tr:first-child td {
    border-top: none;
}
#marcarExamesform table.cruises th { 
    border: 1px solid #999; 
	padding: 8px 4px;
	background: #646473;
	color: white;
    }
#marcarExamesform table.cruises td { 
  	background: #eee; 
  	overflow: hidden; 
}
#marcarExamesform div.scrollableContainer { 
    position: relative; 
    width: 530px; 
    padding-top: 0px;     
    border: 0px solid #999;
    background: #646473;
}
#marcarExamesform div.scrollingArea { 
    height: 337px; 
    overflow: auto; 
}
#marcarExamesform table.scrollable thead tr {
    left: 0px; 
    top: 0;
    position: absolute;  
}
#marcarExamesform table.cruises .analise  div { width: 225px; }
#marcarExamesform table.cruises .preco    div { width: 140px; }
#marcarExamesform table.cruises .accao    div { width: 120px;  text-align:center; }

#btn-middle-consultas{
	width: auto;
	position: absolute;
	bottom: 40%;
	left: 45%;
	padding: 15px 2em 10px;
	border: solid 1px #ccc;
	border-radius: 6px;
}
#btn-middle-consultas input.btn-lista-consultas{
	padding: 8px 17px;
	min-width: 80px;
	color: #222;
	font-size: 13px;
	text-align: center;
	font-weight: normal;
	font-variant: small-caps;	
	border: solid 1px #aaa;
	border-radius: 2px;
	cursor: pointer;
	margin-bottom: 5px;
}

#btn-middle-exames{
	width: auto;
	position: absolute;
	bottom: 10%;
	left: 45%;
	padding: 15px 2em 10px;
	border: solid 1px #ccc;
	border-radius: 6px;
}
#btn-middle-exames input.btn-lista-exame{
	padding: 8px 17px;
	min-width: 80px;
	color: #222;
	font-size: 13px;
	text-align: center;
	font-weight: normal;
	font-variant: small-caps;	
	border: solid 1px #aaa;
	border-radius: 2px;
	cursor: pointer;
	margin-bottom: 5px;
}


/*Beneficios css*/

.beneficios{
	width: auto;
	height: auto;
	overflow: hidden;
	float: right;
}
.beneficios tbody img{
	width: 18px;
	height: 18px;
	vertical-align: middle;
	padding-right: 5px;
}
.beneficios tbody a{
	color: #333;
	vertical-align: middle;
	font-size: 12px;
}
.beneficios tbody a:HOVER{
	color: #666;
}
#beneficiosform fieldset{
