#beneficiosform fieldset.qtd{
 margin-top: 10px;
 padding: 0.8em 1em 0.2em;
}
#beneficiosform fieldset .floatRight{
 width: 100%;
 float: right;
}

#beneficiosform fieldset.qtd label{
 font-size: 14px;
 color: #444;
 text-align: left;
 margin: 0 6px;
 text-transform: uppercase;
 font-weight: bold;
}
#beneficiosform fieldset.qtd input[type=text] {
  color: #666;
  font-size: 14px;
  padding: 5px 12px;
  font-weight: normal;
  width: 80px;
  margin-bottom: 8px;
  background: #F2F2F2;
  border: solid 1px #ccc;
}

#beneficiosform fieldset.qtd input.quant {
  width: 55px;
}
#beneficiosform .btns-beneficios{
	float: right;
	right: 18px;
	top: 10px;
	margin-bottom: 20px;
	position: relative;
}
#beneficiosform input.btn-beneficio{
	padding: 8px 12px;
	color: #222;
	font-size: 13px;
	font-weight: normal;
	font-variant: small-caps;
	text-indent: 20px;
	border: solid 1px #ccc;
	margin: 0 10px 0 0;
	background: url('../img/Save-icon.png') #F2F2F2;
	background-repeat: no-repeat;
	background-size: 22px;
	background-position: 7px;
	border-radius: 2px;
	cursor: pointer;
}
#beneficiosform input.btn-limpar-beneficio{
	padding: 8px 10px;
	color: #222;
	font-size: 13px;
	font-weight: normal;
	font-variant: small-caps;
	text-indent: 20px;
	border: solid 1px #ccc;
	margin: 0;
	background: url('../img/Eraser-icon.png') #F2F2F2;
	background-repeat: no-repeat;
	background-size: 20px;
	background-position: 7px;
	border-radius: 2px;
	cursor: pointer;
}

