/*************************
*******Janela modal CSS******
**************************/
.modalDialog {
	 position: fixed;
	 font-family: Arial, Helvetica, sans-serif;
	 top: 14%;
	 right: 0;
	 bottom: 0;
	 left: 0;
	 background: rgba(0,0,0,0.5);
	 z-index: 99999;
	 opacity:0;
	 -webkit-transition: opacity 400ms ease-in;
	 -moz-transition: opacity 400ms ease-in;
	 transition: opacity 400ms ease-in;
	 pointer-events: none;
	 overflow:scroll; /* activa scroll da pana se a modal for muito comprida */
 }
	
 .modalDialog:target {
 opacity:1;
 pointer-events: auto;
 }
a.close{
	 color: #000;
	 line-height: 25px;
	 position: absolute;
	 right: 25px;
	 top: 10px;
	 width: auto;
	 text-decoration: none;
	 font-weight: bold;
	 font-size: 12px;
 }
	
a.close:hover { color: #444; }

