/*table scroll*/

table.cruises { 
    font-family: 'Open Sans', sans-serif;
    font-size: 13px;
	font-weight: bold;
	cellspacing: 0; 
	border-collapse: collapse; 
    width: 535px;    
    }
table.cruises td { 
    border-left: 1px solid #999; 
    border-top: 1px solid #999;  
    border: 1px solid #ccc;  
	padding: 9.3px 4px;
	text-align: center;
	font-size: 13px;
	font-weight: bold;
	background: #F2F2F2; 
	overflow: hidden;
}
table.cruises tr:first-child td {
    border-top: none;
}
table.cruises th { 
    border: 1px solid #999; 
	padding: 8px 4px;
	background: #646473;
	color: white;
    }
table.cruises td { 
  	background: #eee; 
  	overflow: hidden; 
}
table.cruises tbody {
	top: 2.7em;
    height: 208px; 
    width: 611px; 
    overflow: auto; 
    position: absolute;	
}
div.scrollableContainer { 
    position: relative; 
    width: 611px; 
    padding-top: 0px;     
    border: 0px solid #999;
    background: #646473;
}
div.scrollingArea { 
    height: 243px; 
    overflow: auto; 
}
table.scrollable thead tr {
    left: 0px; 
    top: 0;
    position: absolute;
}
table.cruises .name     div { width: 280px; }
table.cruises .name_exa     div { width: 464px; }/*th Exames css*/
table.cruises .servico div { width: 175px; }
table.cruises .preco    div { width: 111px;  text-align:center; }
  
