.linedwrap {
	border: 1px solid #c0c0c0;
	padding: 0 0 2px 0;
	margin-bottom: 10px;	
}
.linedtextarea {
	padding: 0px;
	margin: 0px;
}
.linedtextarea textarea, .linedwrap .codelines .lineno {
	font-size: 10pt;
	font-family: monospace;
	line-height: normal !important;
}
.linedtextarea textarea {
	padding-right: 0.3em;
	padding-top: 0.3em;
	outline: none;
	border: 0;
}
.linedwrap .lines {
	margin-top: 0px;
	max-width: 30px;
	min-height: 128px;
	float: left;
	overflow: hidden;
	border-right: 1px solid #c0c0c0;
