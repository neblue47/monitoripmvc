
.widget-proc{
  background-image: none;
  background-color: #fcfcfc;
  border-bottom-color: #d9d9d9;
  padding: 10px;
  height: auto;
  overflow: hidden;
  margin-top: 10px; 
  margin-bottom: 5px; 
}
.widget-proc{
  border: 1px solid #CDCDCD;
  height: auto;
  -webkit-box-shadow: 0 1px 0 #ffffff inset;
  box-shadow: 0 1px 0 #ffffff inset;
  border-radius: 0;
}
.widget-receit{
  margin-bottom: 10px;
  border-right: 1px solid #CDCDCD;
  border-left: 1px solid #CDCDCD;
  border-bottom: 1px solid #CDCDCD;
  height: auto;
  -webkit-box-shadow: 0 1px 0 #ffffff inset;
  box-shadow: 0 1px 0 #ffffff inset;
  border-radius: 0;
}

/* -------- Tab  ----------*/
/* For Tratamento */
#fromSinaisVitais, #fromEvolucao {
	display:none;
}
/* For Consultorio */
#fromInternamento, #fromMedicacao, #fromRecomendacoes, #fromSuspensos, #fromExameInt {
	display:none;
}
/* For Antecedentes */
#fromAntecedentesFamiliar{
	display:none;
}
.procediment {
  background:#ffffff;
  border-radius: 0;
}

.procediment-tab-menu ul {
  border: medium none;
  list-style: none outside none;
  margin-bottom: 15px;
  padding: 0;
}

.procediment-tab-menu ul li {
  background: #34495e;
  color: #fff;
  cursor: pointer;
  font-family: ramblabold;
  font-size: 12px;
  padding: 8px 0px;
  text-transform: uppercase;
  transition: all 0.9s ease 0s;
  -moz-transition: all 0.9s ease 0s;
  -webkit-transition: all 0.9s ease 0s;
  -o-transition: all 0.9s ease 0s;
  text-align:center;
  opacity: 0.7;
}

.procediment-tab-menu ul li:first-child {
  background: #34495e;
  color: #fff;
  opacity: 1;
}
